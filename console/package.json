{
  "name": "fusion-access-console",
  "version": "0.0.1",
  "description": "Console plugin for OpenShift Fusion Access Operator",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/openshift-storage-scale/openshift-fusion-access-operator.git",
    "directory": "console"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "codegen": "scripts/codegen.sh",
    "codegen:kubernetes-types": "scripts/codegen.sh generate_kubernetes_types",
    "codegen:openshift-types": "scripts/codegen.sh generate_openshift_types",
    "codegen:plugin-types": "scripts/codegen.sh generate_plugin_types",
    "build": "npm run clean && NODE_ENV=production npm run ts:webpack -- -c config/webpack.config.ts",
    "build:dev": "npm run clean && npm run ts:webpack -- -c config/webpack.config.ts",
    "i18n": "scripts/i18n.sh",
    "lint": "biome check --changed",
    "lint:fix": "biome check --write --changed",
    "format": "biome format --changed",
    "format:fix": "biome format --write --changed",
    "start": "concurrently --kill-others -c auto -n \"console,wds\" -p \"{name}:{pid}\" \"npm run start:console\" \"npm run start:wds\"",
    "start:wds": "npm run ts:webpack -- serve --progress -c config/webpack.config.ts",
    "start:console": "CONSOLE_VERSION=4.19.0 scripts/console.sh start",
    "force-update:console": "scripts/console.sh force_update",
    "test": "scripts/test.sh run",
    "test:report": "scripts/test.sh generate_report",
    "cy:open": "cypress open -p integration-tests",
    "ts:webpack": "node -r ts-node/register ./node_modules/.bin/webpack"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@patternfly/react-table": "^6.2.2",
    "@patternfly/react-tokens": "^6.2.2",
    "convert": "^5.9.0",
    "immer": "^10.1.1",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@biomejs/biome": "2.3.4",
    "@cypress/webpack-preprocessor": "^5.15.5",
    "@openapitools/openapi-generator-cli": "~2.23.0",
    "@openshift-console/dynamic-plugin-sdk": "~4.19.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "~4.19.0",
    "@patternfly/react-core": "^6.2.2",
    "@patternfly/react-icons": "^6.2.2",
    "@patternfly/react-styles": "^6.2.2",
    "@types/node": "^22.0.0",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^17.0.37",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "^8.0.2",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^6.7.1",
    "cypress": "^12.17.4",
    "cypress-multi-reporters": "^1.6.2",
    "globals": "^16.0.0",
    "i18next": "^24.2.3",
    "i18next-parser": "^9.3.0",
    "mocha": "^10.5.1",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "msw": "^2.8.6",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "~11.12.0",
    "react-router": "~5.3.0",
    "react-router-dom": "~5.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "~5.75.0",
    "webpack-cli": "~5.1.0",
    "webpack-dev-server": "~5.1.0"
  }
}
